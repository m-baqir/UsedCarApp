
@{
    ViewBag.Title = "Create";
}
<a class="createnew" href="/Car/List">Back to car list</a>
<h2>Create New Car</h2>

<form action="/Car/Create" method="post">
    
    <div>
        <label>Car Year:</label> <input name="Year" type="number" />
    </div>
    <div>
        <label>Car Make:</label> <input name="Make" type="text" />
    </div>
    <div>
        <label>Car Model:</label> <input name="Model" type="text" />
    </div>
    
    <div>
        <input type="submit" value="Create Car"/>
    </div>
</form>

